page {
  width: 100%;
  height: 100%;
}
view, text {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  font-size: 30rpx;
}
.fl {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.fr {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.fc {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.fb {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.apps {
  width: 100%;
  height: 100%;
}
.fix-results {
  position: fixed;
  bottom: 92rpx;
  width: 100%;
  height: 100%;
}
.fix-results .mask {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.6);
}
.fix-results .fix-main {
  position: absolute;
  width: 100%;
  height: 50%;
  bottom: 0;
  left: 0;
  background: #fff;
  overflow: auto;
  padding: 20rpx;
}
.fix-results .titile {
  position: relative;
}
.fix-results .titile > text {
  color: #000;
  font-size: 28rpx;
  height: 30rpx;
  line-height: 30rpx;
  margin-top: 30rpx;
  padding-left: 30rpx;
}
.fix-results .list {
  display: block;
  padding-top: 20rpx;
}
.fix-results .list .list-item {
  margin-left: 22rpx;
  position: relative;
  overflow: hidden;
  margin-bottom: 10rpx;
  display: inline-block;
}
.fix-results .list .list-item .close {
  position: absolute;
  width: 22rpx;
  height: 23rpx;
  top: 17rpx;
  right: 27rpx;
}
.fix-results .list .list-item .list-content {
  background: #dae6fd;
  color: #4580f6;
  font-size: 26rpx;
  height: 55rpx;
  line-height: 54rpx;
  padding-left: 20rpx;
  padding-right: 55rpx;
  border-radius: 30rpx;
  max-width: 340rpx;
  white-space: pre-wrap;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  -ms-text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  display: inline-block;
}
.fx-illList {
  width: 100%;
  height: 100%;
  background: #fff;
  position: relative;
  overflow: hidden;
}
.fx-illList .fx-tips {
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  border-bottom: 1px solid #dddddd;
  color: #4580f6;
  font-weight: bold;
  padding-left: 27rpx;
}
.fx-illList .fx-btns {
  height: 92rpx;
  position: fixed;
  bottom: 0;
  width: 100%;
  border-top: 1px solid #ddd;
  display: table-cell;
}
.fx-illList .fx-btns .btn {
  width: 50%;
  height: 100%;
  float: left;
  text-align: center;
  font-size: 30rpx;
}
.fx-illList .fx-btns .preBtn {
  color: #000;
  background: #fff;
  line-height: 92rpx;
}
.fx-illList .fx-btns .nextBtn {
  background: #4580f6;
  color: #fff;
  line-height: 92rpx;
}
.fx-illList .illList {
  height: 100%;
}
.fx-illList .point-list {
  height: 100%;
  padding-bottom: 180rpx;
  width: 330rpx;
  background: #f6f6f6;
  overflow: auto;
  float: left;
}
.fx-illList .point-list .activeItem {
  background: #ffffff;
  display: inline-block;
  color: #4580f6;
}
.fx-illList .point-list .item > text {
  width: 100%;
  display: inline-block;
  padding: 20rpx;
  padding-right: 10rpx;
  line-height: 40rpx;
  font-size: 28rpx;
}
.fx-illList .child-list {
  height: 100%;
  padding-bottom: 180rpx;
  width: 420rpx;
  background: #fff;
  overflow: auto;
  float: right;
}
.fx-illList .child-list .disease {
  position: relative;
  border-bottom: 1px solid #ddd;
  line-height: 40rpx;
}
.fx-illList .child-list .disease .diseaseItem {
  width: 100%;
  font-size: 28rpx;
  padding: 20rpx;
  padding-right: 60rpx;
}
.fx-illList .child-list .disease .diseaseItem.active {
  color: #4580f6;
}
.fx-illList .child-list .disease .choseIcon {
  width: 40rpx;
  height: 22rpx;
  position: absolute;
  color: #1b8bf9;
  padding: 0;
  right: 10rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.fx-illList .child-list .disease .choseIcon > image {
  width: 30rpx;
  height: 22rpx;
  position: absolute;
}
.fx-illList .child-list .disease .choseIcon.active {
  opacity: 1;
}

